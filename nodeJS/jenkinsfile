pipeline {
    agent any
    environment {
        SONARQUBE_SERVER = 'SonarQubeLocal'
    }
    stages {
        stage('Checkout') {
            steps { checkout scm }
        }
        stage('SonarQube Analysis') {
            steps {
                withSonarQubeEnv(SONARQUBE_SERVER) {
                    bat "sonar-scanner.bat -Dsonar.projectKey=AI-Emergency -Dsonar.sources=."
                }
            }
        }
    }
}
